        "squizlabs/php_codesniffer" : ">=2.3.4",
        "phpmd/phpmd" : ">=2.3.2",
        "sebastian/phpcpd" : ">=2.0.2",
        "pdepend/pdepend" : ">=2.0.6",
        "phploc/phploc" : ">=2.1.1",
        "phpdocumentor/phpdocumentor" : ">=2.8.5"
	},
	"minimum-stability" : "stable"
